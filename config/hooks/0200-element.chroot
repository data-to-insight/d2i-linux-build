#!/bin/bash
set -e
wget -qO /usr/share/keyrings/element-io-archive-keyring.gpg \
  https://packages.element.io/debian/element-io-archive-keyring.gpg
echo \
  "deb [signed-by=/usr/share/keyrings/element-io-archive-keyring.gpg] \
  https://packages.element.io/debian/ default main" \
  > /etc/apt/sources.list.d/element-io.list
apt update
apt install -y element-desktop
